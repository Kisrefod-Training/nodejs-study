doctype html
head
    meta(charset='UTF-8')
    title Parsed Github Data
    link(rel='stylesheet', href='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css', integrity='sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2', crossorigin='anonymous')
table.table
    thead.thead-dark
        tr
            th(scope='col') #
            th(scope='col') User
            th(scope='col') Type
            th(scope='col') Link
            th(scope='col') ID
    tbody
        - let count = 1;
        - const commitsAndPR = parsedData;
        each obj in commitsAndPR
            tr
                th(scope='row') #{count++}
                td= obj.user
                td= obj.type
                td= obj.URL
                td= obj.ID
